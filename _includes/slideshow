	// Add/delete additional slides as desired.
	var slides=new Array()
	slides[0]='<img src="/images/slideshow/small-1.jpg" width=533 height=140 border=0 alt="" usemap="#1">'
	slides[1]='<img src="/images/slideshow/small-2.jpg" width=533 height=140 border=0 alt="" usemap="#2">'
